{
  "name": "@modernpoacher/modernpoacher",
  "version": "1.1.122",
  "description": "@ModernPoacher/modernpoacher",
  "private": true,
  "type": "module",
  "author": {
    "name": "Jonathan Perry for Modern Poacher Limited",
    "email": "modernpoacher@modernpoacher.com",
    "url": "https://modernpoacher.com"
  },
  "scripts": {
    "build": "DEBUG=modernpoacher* webpack",
    "build:clean": "rimraf pub/assets/css/*.css pub/assets/css/*.css.map pub/assets/js/*.cjs pub/assets/js/*.mjs pub/assets/js/*.cjs.map pub/assets/js/*.mjs.map 2> /dev/null",
    "build:watch": "npm run build -- -w",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint src/sass",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "eslint .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "nvm": "bash nvm.sh",
    "prebuild": "npm run nvm",
    "prepare": "husky",
    "prettier": "prettier src/**/*.scss --write",
    "transform": "DEBUG=modernpoacher* ./transform.sh"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@sequencemedia/eslint-config-standard": "^0.2.71",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "babel-plugin-minify-dead-code-elimination": "^0.5.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "core-js": "^3.44.0",
    "css-loader": "^7.1.2",
    "debug": "^4.4.1",
    "eslint": "^9.31.0",
    "glob-all": "^3.3.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "mini-css-extract-plugin": "^2.9.2",
    "perfectionist": "^2.4.0",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.6.2",
    "remove-files-webpack-plugin": "^1.5.0",
    "rimraf": "^6.0.1",
    "sass": "^1.89.2",
    "sass-loader": "^16.0.5",
    "stylelint": "^16.22.0",
    "stylelint-config-recommended-scss": "^15.0.1",
    "stylelint-scss": "^6.12.1",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.100.2",
    "webpack-cli": "^6.0.1"
  }
}
