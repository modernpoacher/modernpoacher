{
  "name": "@modernpoacher/modernpoacher",
  "version": "1.0.89",
  "description": "@ModernPoacher/modernpoacher",
  "private": true,
  "type": "module",
  "author": "Jonathan Perry for Modern Poacher Limited <modernpoacher@modernpoacher.com>",
  "scripts": {
    "build:clean": "cross-env NODE_ENV=production concurrently -r \"npm run build:js:clean\" \"npm run build:css:clean\"",
    "build": "cross-env NODE_ENV=production concurrently -r \"npm run build:js\" \"npm run build:css\"",
    "build:watch": "cross-env NODE_ENV=production concurrently -r \"npm run build:js:watch\" \"npm run build:css:watch\"",
    "build:js:clean": "rimraf pub/assets/js/*.js pub/assets/js/*.js.map",
    "build:js": "npm run webpack",
    "build:js:watch": "npm run webpack -- -w",
    "build:css:clean": "npm run gulp -- build:css:clean",
    "build:css": "npm run gulp -- build:css",
    "build:css:watch": "npm run gulp -- build:css:watch",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "cross-env NODE_ENV=production eslint . --ext .mjs,.cjs,.js",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "cross-env NODE_ENV=production stylelint src/sass",
    "lint:css:fix": "npm run lint:css -- --fix",
    "nvm": "bash nvm.sh",
    "webpack": "webpack",
    "gulp": "gulp --color",
    "prepare": "husky install"
  },
  "dependencies": {
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.9",
    "@babel/eslint-parser": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@modernpoacher/hooks": "^1.0.427",
    "@sequencemedia/gulp": "^1.1.51",
    "@sequencemedia/gulp-clean-css": "^1.0.21",
    "@sequencemedia/gulp-css-purge": "^1.1.14",
    "@sequencemedia/gulp-debug": "^1.0.25",
    "@sequencemedia/gulp-postcss": "^1.0.22",
    "@sequencemedia/gulp-sass": "^1.0.20",
    "@sequencemedia/gulp-sourcemaps": "^1.0.20",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.3",
    "babel-plugin-minify-dead-code-elimination": "^0.5.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.0",
    "core-js": "^3.32.0",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "del": "^7.0.0",
    "eslint": "^8.46.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "husky": "^8.0.3",
    "postcss": "^8.4.27",
    "postcss-scss": "^4.0.6",
    "rimraf": "^5.0.1",
    "sass": "^1.64.2",
    "stylelint": "^15.10.2",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-scss": "^5.0.1",
    "terser-webpack-plugin": "^5.3.9",
    "vinyl-paths": "^5.0.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "imports": {
    "#build/gulp/*": "./build/gulp/*.mjs",
    "#build/gulp": "./build/gulp/index.mjs"
  }
}
