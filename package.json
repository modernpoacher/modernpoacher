{
  "name": "@modernpoacher/modernpoacher",
  "version": "0.0.20",
  "description": "@ModernPoacher/modernpoacher",
  "private": true,
  "author": "Jonathan Perry for Modern Poacher Limited <modernpoacher@modernpoacher.com>",
  "scripts": {
    "build:clean": "cross-env NODE_ENV=production gulp build:clean",
    "build": "cross-env NODE_ENV=production gulp build",
    "build:watch": "cross-env NODE_ENV=production gulp build:watch",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "cross-env NODE_ENV=production eslint .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "cross-env NODE_ENV=production stylelint src/sass",
    "lint:css:fix": "npm run lint:css -- --fix",
    "pre-commit": "npm run lint && npm run build && git add pub"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.3",
    "@babel/register": "^7.10.3",
    "autoprefixer": "^9.8.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "debug": "^4.1.1",
    "del": "^5.1.0",
    "eslint": "^7.3.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-css-purge": "^3.0.9",
    "gulp-debug": "^4.0.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "husky": "^4.2.5",
    "postcss-scss": "^2.1.1",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.18.0",
    "vinyl-paths": "^3.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
